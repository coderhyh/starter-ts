{
  "name": "starter-ts",
  "type": "module",
  "version": "0.0.1",
  "description": "npm库的起始模板",
  "author": "coderhyh <coderhyh@gmail.com>",
  "license": "MIT",
  "homepage": "https://coderhyh.github.io/starter-ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coderhyh/starter-ts"
  },
  "bugs": "https://github.com/coderhyh/starter-ts/issues",
  "keywords": [
    "starter-ts"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.js"
    },
    "./es": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js"
    },
    "./lib": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js"
    },
    "./*": "./*"
  },
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "engines": {
    "node": ">=18.18.2"
  },
  "scripts": {
    "build": "npm run clean && vue-tsc --noEmit && vite build",
    "clean": "rimraf dist lib es types",
    "release": "bumpp",
    "test": "vitest -watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint packages/ --fix",
    "postinstall": "npx simple-git-hooks",
    "commit": "git add . && cz",
    "prettier": "prettier --write .",
    "docs:dev": "vitepress dev packages",
    "docs:build": "vitepress build packages",
    "docs:preview": "vitepress preview packages"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@types/node": "^18.19.80",
    "@unocss/eslint-plugin": "66.1.0-beta.5",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.4.0",
    "bumpp": "^10.1.0",
    "element-plus": "^2.9.6",
    "eslint": "^9.22.0",
    "eslint-plugin-format": "^0.1.3",
    "lint-staged": "^15.5.0",
    "rimraf": "^5.0.10",
    "simple-git-hooks": "^2.11.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "unocss": "^66.0.0",
    "unplugin-auto-import": "^19.1.1",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.2",
    "vite-plugin-build": "^0.10.0",
    "vitepress": "1.6.3",
    "vitest": "^3.0.8",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx}": "pnpm lint:script",
    "*.{css,less,vue,html}": "pnpm lint:style"
  }
}
